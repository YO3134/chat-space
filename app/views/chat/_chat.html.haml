.chat__header
  .left-header
    .left-header__title
      = @group.name
    %ul.left-header__members
      Members:
      %li.member
        - @group.group_users.each |group_user|
          =group_user.user.name
  .right-header
    .right-header__button
      = link_to edit_group_path(@group) do
        %input{ value: "Edit"}
